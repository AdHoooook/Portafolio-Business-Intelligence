      var $ = window.jQuery;
      $(document).ready(function() {
         $('.web-sii #menu-lateral li.main').click(function() {
            var $nclase = $(this).attr('id');
            $nclase = $nclase.replace("padre_", "");
            if($('.web-sii #menu-lateral ul#' + $nclase).hasClass('in'))
            {
               event.preventDefault ? event.preventDefault() : (event.returnValue = false);
               var $selClosedMain = $(this).find('span');
               if($selClosedMain.hasClass('glyphicon-menu-up')){
                  $selClosedMain.removeClass('glyphicon-menu-up');
                  $selClosedMain.addClass('glyphicon-menu-down');
   			   }
            }
         });
      });
   };
   document.getElementsByTagName("head")[0].appendChild(script);
})();
