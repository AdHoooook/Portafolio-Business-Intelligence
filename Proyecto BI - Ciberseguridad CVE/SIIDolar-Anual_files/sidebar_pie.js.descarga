function abreMenu(id_padre, id_hijo, id_abuelo){   
   $("#"+id_padre).addClass('in');
   $("#"+id_padre).siblings('a').find('span').toggleClass('glyphicon-menu-down glyphicon-menu-up');
   $("#"+id_hijo).addClass('active');
   $("#"+id_padre).siblings('a').attr('style','color:#eb510d; border-top: 2px solid #eb510d; font-weight:bold;');
   $("#"+id_padre).attr('style','border-bottom: 2px solid #eb510d;');
   $("#"+id_abuelo).addClass('in');
}

function writeOptionsYears(parId){
	var desde = 2013;
	var hasta;
   switch(parId) {
      case 0:   // Circulares
         hasta = 2020;
         break;
      case 1:   // Resoluciones
         hasta = 2020;
         break;
      case 2:   // jurisprudencia administrativa (renta)
		   desde = 2009
         hasta = 2020;
         break;
      case 3:   // jurisprudencia administrativa (ventas y servicios)
		   desde = 2009
         hasta = 2020;
         break;
      case 4:   // jurisprudencia administrativa (otras)
		   desde = 2009
         hasta = 2020;
         break;
      case 12:   // UTM - UTA - IPC
         hasta = 2020;
         break;
      case 18:   // Resoluciones de personal
         hasta = 2020;
         break;
      case 23:   // Actos y Resoluciones de las direcciones regionales y grandes contribuyentes
         hasta = 2020;
         break;
      case 30:  // Impuesto Único de Segunda Categoría (Art.52)
		   desde = 2017;
         hasta = 2020;
         break;
      case 101:  // impuesto único segunda categoría
         hasta = 2020;
         break;
      case 102:  // Odepa
         hasta = 2020;
         break;
      case 103:  // Fondo de Estabilización de Precios del Petróleo
         hasta = 2020;
         break;
      case 104:  // Dólar
         hasta = 2020;
         break;
      case 106:  // U. F.
         hasta = 2020;
         break;
      case 108:  // corrección monetaria
         hasta = 2020;
         break;
      case 2012:  // Impuesto al petróleo (Componentes base y variable) - Mepco
         hasta = 2020;
         break;
   }

   for (var i = hasta; i >= desde ; i--) {
	   document.write("   <option value='"+i+"'>"+i+"</option>");
   }
}


(function() {
